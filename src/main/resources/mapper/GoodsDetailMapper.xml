<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.orderbackv1.mapper.GoodsDetailMapper">

    <resultMap id="BaseResultMap" type="com.example.orderbackv1.domain.GoodsDetail">
            <id property="goodsId" column="goods_id" jdbcType="INTEGER"/>
            <result property="goodsName" column="goods_name" jdbcType="VARCHAR"/>
            <result property="goodsPrice" column="goods_price" jdbcType="DECIMAL"/>
            <result property="goodsNumber" column="goods_number" jdbcType="INTEGER"/>
            <result property="goodsWeight" column="goods_weight" jdbcType="INTEGER"/>
            <result property="goodsIntroduce" column="goods_introduce" jdbcType="VARCHAR"/>
            <result property="goodsDeleted" column="goods_deleted" jdbcType="TINYINT"/>
            <result property="goodsState" column="goods_state" jdbcType="INTEGER"/>
            <result property="goodsCat" column="goods_cat" jdbcType="VARCHAR"/>
            <result property="picUrl" column="pic_url" jdbcType="VARCHAR"/>
    </resultMap>

    <sql id="Base_Column_List">
        goods_id,goods_name,goods_price,
        goods_number,goods_weight,goods_introduce,
        goods_deleted,goods_state,goods_cat,
        pic_url
    </sql>
</mapper>
