<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.orderbackv1.mapper.CategoryMapper">

    <resultMap id="BaseResultMap" type="com.example.orderbackv1.domain.Category">
            <id property="catId" column="cat_id" jdbcType="INTEGER"/>
            <result property="catName" column="cat_name" jdbcType="VARCHAR"/>
            <result property="catDeleted" column="cat_deleted" jdbcType="TINYINT"/>
            <result property="catIcon" column="cat_icon" jdbcType="VARCHAR"/>
    </resultMap>

    <sql id="Base_Column_List">
        cat_id,cat_name,cat_deleted,
        cat_icon
    </sql>
</mapper>
